<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.UserVO" alias="user"/>
		<typeAlias type="vo.AdminVO" alias="admin"/>
		<typeAlias type="vo.GongjiVO" alias="gongji"/>
		<typeAlias type="vo.BoardVO" alias="board"/>
		<typeAlias type="vo.Board_ComVO" alias="board_com"/>
		<typeAlias type="vo.RentbookVO" alias="rentbook"/>
		<typeAlias type="vo.RentVO" alias="rent"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/user.xml" />
		<mapper resource="config/mybatis/mapper/admin.xml" />
		<mapper resource="config/mybatis/mapper/board.xml" />
		<mapper resource="config/mybatis/mapper/rentbook.xml" />
		<mapper resource="config/mybatis/mapper/rent.xml" />
	</mappers>
</configuration>